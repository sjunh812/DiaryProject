    <!-- 시스템 서체 테마(NoActionBar) -->
    <!-- -->
    <style name="Theme.BasicDiaryProject" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorAccent">@color/pastel_700</item>
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/background</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- API 23 이상부터 가능(상태바 위의 상태표시줄 색상) -->
        <item name="android:windowLightStatusBar">true</item>
        <!-- Customize your theme here. -->
        <item name="toolbarStyle">@style/ToolbarStyle</item>
        <item name="android:buttonStyle">@android:style/Widget.DeviceDefault.Button.Borderless</item>
        <item name="mcv_headerTextAppearance">@style/customCalenderHeaderFontStyle0</item>
        <item name="mcv_dateTextAppearance">@style/customCalenderHeaderFontStyle0</item>
    </style>
    <style name="customCalenderHeaderFontStyle0">
